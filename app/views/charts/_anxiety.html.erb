<script> 
$(function () {
        $('#container3').highcharts({
            title: {
                text: 'Anxiety - Analytics',
                x: -20 //center
            },
            xAxis: { 
	
                categories: [ 
                              <% @mood_data.each do |t| %> 
                                '<%= t.entry_at.strftime("%m/%d/%y") %>',  
                              <% end %>
                            ]
            },
           	yAxis: {
                categories: ['0', 'Mild', 'Moderate', 'Severe'],
                min: 0, 
								max: 3
            },
            tooltip: {
               
            },
            legend: {
                layout: 'vertical',
                align: 'right',
                verticalAlign: 'middle',
                borderWidth: 0
            },
            series: [{
                data: <%= @mood_data.map{|vv| vv.anxiety }%>
            }
          ]
        });
    });  

</script>
